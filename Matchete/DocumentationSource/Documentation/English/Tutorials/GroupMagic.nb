Cell[TextData[{
 "Some useful group invariants that often show up in QFT calculations are the \
Dynkin index ",
 Cell[BoxData[
  RowBox[{
   ButtonBox["DynkinIndex",
    BaseStyle->"Link",
    ButtonData->"paclet:Matchete/ref/DynkinIndex"], "[", 
   RowBox[{
    StyleBox["alg", "TI"], ",", " ", 
    StyleBox["rep", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "cbada0e4-b0fa-420e-961e-b46b01498af2"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Tr", "[", 
     RowBox[{
      SuperscriptBox["T", "A"], 
      SuperscriptBox["T", "B"]}], "]"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["S", "2"], "(", "R", ")"}], 
     SuperscriptBox["\[Delta]", 
      RowBox[{"A", " ", "B"}]]}]}], TraditionalForm]],ExpressionUUID->
  "21572c25-3a8d-4703-9e35-792ed4b02f67"],
 " for representation ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]],ExpressionUUID->
  "cc1eb74c-023f-4806-ad72-4a93dcc2efab"],
 " ), here normalized to ",
 Cell[BoxData[
  FormBox[
   FractionBox["1", "2"], TraditionalForm]],ExpressionUUID->
  "f317c5f5-4dcb-4dee-b805-ba9c69f8fd0c"],
 " for the fundamental representations, and the quadratic Casimir ",
 Cell[BoxData[
  RowBox[{
   ButtonBox["Casimir2",
    BaseStyle->"Link",
    ButtonData->"paclet:Matchete/ref/Casimir2"], "[", 
   RowBox[{
    StyleBox["alg", "TI"], ",", " ", 
    StyleBox["rep", "TI"]}], "]"}]], "InlineFormula",ExpressionUUID->
  "3884e797-9b5c-4ce9-a012-ff3cbd4ca873"],
 " (",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["T", "A"], 
     SuperscriptBox["T", "A"]}], " ", "=", " ", 
    RowBox[{
     SubscriptBox["C", "2"], "(", "R", ")"}], " "}], TraditionalForm]],
  ExpressionUUID->"fbede722-8a8d-4b34-9632-f6a047a5e3df"],
 "\[DoubleStruckCapitalI]). "
}], "Text",
 CellID->806251118,ExpressionUUID->"73b375e6-acda-4e07-8dec-895bea829c1a"],

