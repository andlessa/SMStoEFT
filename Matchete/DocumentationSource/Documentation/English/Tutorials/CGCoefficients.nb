 RowBox[{"RemoveCG", "[", "t3t3", "]"}], "\[IndentingNewLine]", 
 RowBox[{"DefineCompositeCG", "[", 
  RowBox[{"tf3", ",", 
   RowBox[{"{", 
    RowBox[{"f3", ",", "t3"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "b", ",", "c"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"c", ",", "i", ",", "j"}], "}"}]}], "}"}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"DefineCompositeCG", "[", 
  RowBox[{"td3", ",", 
   RowBox[{"{", 
    RowBox[{"d3", ",", "t3"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "b", ",", "c"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"c", ",", "i", ",", "j"}], "}"}]}], "}"}]}], "]"}]}], "Input",
 CellLabel->"In[36]:=",
 CellID->695079337,ExpressionUUID->"c7183d38-53d9-4128-b685-b118ecd9ed9b"]
}, Open  ]],

